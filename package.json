        "start:signy": "node ./dist/apps/signy/main.js",
        "__________dev infra__________": "__________dev infra__________",
        "start:app": "nx run-many --target=serve --all --parallel --maxParallel=7",
        "start:dev:gateway": "nx run gateway:serve",
        "start:dev:users": "nx run users:serve",
        "start:dev:otp": "nx run otp:serve",
        "start:dev:email": "nx run email:serve",
        "start:dev:company": "nx run company:serve",
        "start:dev:signy": "nx run signy:serve",
        "__________database__________": "__________database__________",
        "migrate:make": "nx run db:migrate-make --name",
        "migrate:latest": "nx run db:migrate-latest",
        "migrate:rollback": "nx run db:migrate-rollback",
        "seed:run": "nx run db:seed-run",
        "__________test__________": "__________test__________",
        "jest": "jest",
        "test": "npm run jest -- --config=./jest.all-config.js --forceExit --passWithNoTests --reporters=default --reporters=jest-junit --includeConsoleOutput=true"
