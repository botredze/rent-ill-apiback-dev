        "build:app": "nx affected --target=build --all",
        "start": "nx serve",
        "__________prod infra__________": "__________prod infra__________",
        "start:gateway": "node ./dist/apps/gateway/main.js",
        "start:users": "node ./dist/apps/users/main.js",
